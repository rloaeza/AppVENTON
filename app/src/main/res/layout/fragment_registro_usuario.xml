<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/japanbackground"
    tools:context=".RegistroUsuario">

    <EditText
        android:id="@+id/nombre"
        android:layout_width="162dp"
        android:layout_height="43dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="105dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="18dp"
        android:backgroundTint="@color/colorBotones"
        android:ems="10"
        android:hint="@string/txtNombre"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:textColor="@color/colorBotones"
        android:textColorHint="@color/colorBotones"
        app:layout_constraintBottom_toTopOf="@+id/apellidoM"
        app:layout_constraintEnd_toStartOf="@+id/apellidoP"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:targetApi="o" />

    <EditText
        android:id="@+id/apellidoP"
        android:layout_width="151dp"
        android:layout_height="44dp"
        android:layout_marginTop="104dp"
        android:layout_marginEnd="46dp"
        android:layout_marginBottom="100dp"
        android:backgroundTint="@color/colorBotones"
        android:ems="10"
        android:hint="@string/txtApellidoP"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:textColor="@color/colorBotones"
        android:textColorHint="@color/colorBotones"
        app:layout_constraintBottom_toTopOf="@+id/email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:targetApi="o" />

    <EditText
        android:id="@+id/apellidoM"
        android:layout_width="170dp"
        android:layout_height="46dp"
        android:layout_marginStart="14dp"
        android:layout_marginEnd="227dp"
        android:layout_marginBottom="36dp"
        android:backgroundTint="@color/colorBotones"
        android:ems="10"
        android:hint="@string/txtApellidoM"
        android:importantForAutofill="no"
        android:inputType="textPersonName"
        android:textColor="@color/colorBotones"
        android:textColorHint="@color/colorBotones"
        app:layout_constraintBottom_toTopOf="@+id/email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:targetApi="o" />

    <EditText
        android:id="@+id/email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        android:layout_marginEnd="184dp"
        android:layout_marginBottom="22dp"
        android:backgroundTint="@color/colorBotones"
        android:ems="10"
        android:hint="@string/txtUsuario"
        android:importantForAutofill="no"
        android:inputType="textEmailAddress"
        android:textColor="@color/colorBotones"
        android:textColorHint="@color/colorBotones"
        app:layout_constraintBottom_toTopOf="@+id/contrasena"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:targetApi="o" />

    <EditText
        android:id="@+id/contrasena"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="182dp"
        android:layout_marginBottom="197dp"
        android:backgroundTint="@color/colorBotones"
        android:ems="10"
        android:hint="@string/txtContra"
        android:importantForAutofill="no"
        android:inputType="textPassword"
        android:textColor="@color/colorBotones"
        android:textColorHint="@color/colorBotones"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:targetApi="o" />

    <Button
        android:id="@+id/button"
        android:layout_width="109dp"
        android:layout_height="48dp"
        android:layout_marginStart="136dp"
        android:layout_marginEnd="166dp"
        android:layout_marginBottom="126dp"
        android:background="@color/colorBackBotones"
        android:text="@string/txtCrearCuenta"
        android:textColor="@color/colorBotones"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</android.support.constraint.ConstraintLayout>
